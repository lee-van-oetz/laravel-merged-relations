{
    "name": "staudenmeir/laravel-merged-relations",
    "description": "Merged Laravel Eloquent relationships",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Staudenmeir",
            "email": "mail@jonas-staudenmeir.de"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.0",
        "staudenmeir/laravel-migration-views": "^1.7"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.1",
        "staudenmeir/eloquent-has-many-deep": "^1.18"
    },
    "autoload": {
        "psr-4": {
            "Staudenmeir\\LaravelMergedRelations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Staudenmeir\\LaravelMergedRelations\\DatabaseServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
