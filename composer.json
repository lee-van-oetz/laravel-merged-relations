{
    "name": "staudenmeir/laravel-merged-relations",
    "description": "Merged Laravel Eloquent relationships",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Staudenmeir",
            "email": "mail@jonas-staudenmeir.de"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/database": "^9.0",
        "staudenmeir/laravel-migration-views": "^1.6"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Staudenmeir\\LaravelMergedRelations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Staudenmeir\\LaravelMergedRelations\\DatabaseServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
